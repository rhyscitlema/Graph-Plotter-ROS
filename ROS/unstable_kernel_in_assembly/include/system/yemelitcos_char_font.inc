;-------------------------------------------------------
; Set a character font
; for character printing
;	in: eax = characater number
;	    ebx = first 32 bits
;	    ecx = second 32 bits
;	    edx = third 32 bits
;-------------------------------------------------------

setCharFont:
	push	eax
	push	ecx
	mov	ecx, 12			; 12 bytes used for every character
	mul	ecx
	add	eax, charFontsList
	mov	[eax], ebx
	mov	[eax+4], ecx
	mov	[eax+8], edx
	pop	ecx
	pop	eax
	ret


